65).\",\r\n        \"functionalArea\": \"\",\r\n        \"newFunctionalityTitle\": \"\",\r\n        \"newFunctionality\": \"\",\r\n        \"businessProcesses\": \"\",\r\n        \"processTasks\": \"\",\r\n        \"webServicesName\": \"\",\r\n        \"securityDomains\": \"\",\r\n        \"currentName\": \"\",\r\n        \"formerName\": \"\",\r\n        \"retired\": \"\",\r\n        \"publishDate\": \"\",\r\n        \"corrected\": \"\",\r\n        \"correctedDate\": \"\"\r\n    }, {\r\n        \"featureJira\": \"FINPRO-16954\",\r\n        \"stoylProductRefId\": \"5a01f3052f1d0e26281cdf57\",\r\n        \"stoylProductCode\": \"WDAY\",\r\n        \"productVersion\": \"29\",\r\n        \"updateType\": \"release\",\r\n        \"feature\": \"\",\r\n        \"functionalArea\": \"Integration\\r\\n\\r\\nProcurement\",\r\n        \"newFunctionalityTitle\": \"Setup Required\",\r\n        \"newFunctionality\": \"With Workday 29, we introduce a new Purchase Order Acknowledgement Inbound integration template, enabling you to import purchase order acknowledgements in the EDI 855 ASC X12 format.\",\r\n        \"businessProcesses\": \"\",\r\n        \"processTasks\": \"\",\r\n        \"webServicesName\": \"\",\r\n        \"securityDomains\": \"\",\r\n        \"currentName\": \"\",\r\n        \"formerName\": \"\",\r\n        \"retired\": \"\",\r\n        \"publishDate\": \"9/9/17\",\r\n        \"corrected\": \"\",\r\n        \"correctedDate\": \"\"\r\n    }, {\r\n        \"featureJira\": \"\",\r\n        \"stoylProductRefId\": \"5a01f3052f1d0e26281cdf57\",\r\n        \"stoylProductCode\": \"WDAY\",\r\n        \"productVersion\": \"29\",\r\n        \"updateType\": \"release\",\r\n        \"feature\": \"Procurement\",\r\n        \"functionalArea\": \"\",\r\n        \"newFunctionalityTitle\": \"\",\r\n        \"newFunctionality\": \"\",\r\n        \"businessProcesses\": \"\",\r\n        \"processTasks\": \"\",\r\n        \"webServicesName\": \"\",\r\n        \"securityDomains\": \"\",\